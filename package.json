{
  "name": "@arielgonzaguer/michi-router",
  "private": false,
  "version": "1.4.0",
  "description": "El router minimalista y simple para React.\nEl objetivo principal de esta herramienta es proporcionar la funcionalidad básica de enrutamiento.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "build:bundle": "tsup src/index.ts --format esm,cjs --minify --dts --out-dir dist --external react --external react-dom"
  },
  "keywords": [
    "enrutado",
    "react",
    "router",
    "typescript",
    "michi",
    "cats",
    "gatos"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@swc/core": "1.13.5",
    "@testing-library/jest-dom": "6.0.0",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.3",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "jsdom": "26.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "6.0.1",
    "tsup": "8.5.0",
    "typescript": "4.9.5",
    "vitest": "3.2.4"
  },
  "author": "Ariel Gonzales Agüero",
  "license": "MIT"
}
